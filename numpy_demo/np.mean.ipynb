    "# axis参数\n",
    "import numpy as np\n",
    "arr=np.reshape(np.arange(0,120),newshape=[2,3,4,5])\n",
    "mean_arr=np.mean(arr,axis=1)\n",
    "\n",
    "print(\"mean_arr的shape为\")\n",
    "print(mean_arr.shape)\n",
    "\n",
    "print(\"\")\n",
    "print(np.mean(arr[0,:,0,0]))\n",
    "print(mean_arr[0,0,0])\n"
   ]
